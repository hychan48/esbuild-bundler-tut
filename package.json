{
  "name": "esbuild-bundler-tut",
  "version": "0.0.1",
  "description": "Binary Bundler Tutorial using ESBuild",
  "main": "index.mjs",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "index": "node index.mjs",
    "build": "esbuild index.mjs --bundle --outfile=builds/build.cjs --format=cjs --platform=node",
    "build:pkg_buildcjs": "pkg builds/build.cjs",
    "build:pkg": "pkg --debug builds/build.cjs --out-path dist"
  },
  "keywords": [],
  "author": "hychan48",
  "license": "MIT",
  "devDependencies": {
    "esbuild": "^0.17.17",
    "pkg": "^5.8.1"
  }}
